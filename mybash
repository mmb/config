export EDITOR=vi

alias update='sudo apt-get update && sudo apt-get upgrade'

GREEN='\e[0;32m'
BLUE='\e[0;34m'
RESET='\e[0m'

export PS1="${debian_chroot:+($debian_chroot)}\u@\h:\[$BLUE\]\w\[$RESET\]\$(git branch 2>/dev/null | sed -n 's/^\* \(.*\)/ git:\[$GREEN\]\1\[$RESET\]/p')\$ "

. ~/mybash_private
